<?xml version="1.0"?>
<robot name="aubo_i5" xmlns:xacro="http://ros.org/wiki/xacro">

 <!-- Defining the colors used in this robot -->
    <material name="Black">
        <color rgba="0 0 0 1"/>
    </material>

    <material name="White">
        <color rgba="1 1 1 1"/>
    </material>

    <material name="Blue">
        <color rgba="0 0 1 1"/>
    </material>

    <material name="Red">
        <color rgba="1 0 0 1"/>
    </material>

    <material name="Grey">
        <color rgba="0.96 0.96 0.96 1"/>
    </material>

    <!-- Constants -->
    <xacro:property name="M_PI" value="3.14159"/>







    <!--/////////////////// DYNAMICS PARAM //////////////////////-->

    <xacro:macro name="I5_Shoulder_Inertia">
        <origin xyz="0.0 0.006 -0.002"
                rpy="0 0 0" />
        <mass value="5.05" />
        <inertia ixx="0.0148"
                 ixy="0"
                 ixz="0"
                 iyy="0.0143"
                 iyz="-0.0"
                 izz="0.0095" />
    </xacro:macro>

    <xacro:macro name="I5_UpperArm_Inertia">
        <origin xyz="0.304 0.0 -0.03"
                rpy="0 0 0" />
        <mass value="11.90" />
        <inertia ixx="0.0305"
                 ixy="0"
                 ixz="0"
                 iyy="0.4505"
                 iyz="0"
                 izz="0.0095" />
    </xacro:macro>

    <xacro:macro name="I5_ForeArm_Inertia">
        <origin xyz="0.415 0.0 0.0992"
                rpy="0 0 0" />
        <mass  value="3.3" />
        <inertia ixx="0.0033"
                 ixy="0"
                 ixz="0.0009"
                 iyy="0.0575"
                 iyz="0"
                 izz="0.0565" />
    </xacro:macro>

    <xacro:macro name="I5_Wrist1_Inertia">
        <origin xyz="0.0 0.0028 -0.003"
                rpy="0 0 0" />
        <mass value="1.62" />
        <inertia ixx="0.0023"
                 ixy="0"
                 ixz="0"
                 iyy="0.0013"
                 iyz="0.0"
                 izz="0.0022" />
    </xacro:macro>

    <xacro:macro name="I5_Wrist2_Inertia">
        <origin xyz="0.0 -0.0028 -0.003"
                rpy="0 0 0" />
        <mass value="1.62" />
        <inertia ixx="0.0023"
                 ixy="0"
                 ixz="0"
                 iyy="0.0013"
                 iyz="0.0"
                 izz="0.0022" />
    </xacro:macro>

    <xacro:macro name="I5_Wrist3_Inertia">
        <origin xyz="0.0 0.0 -0.0071"
                rpy="0 0 0" />
        <mass value="0.40" />
        <inertia ixx="0.013939"
                 ixy="0"
                 ixz="0"
                 iyy="0.013988"
                 iyz="0.0"
                 izz="0.0224092" />
    </xacro:macro>

    <xacro:macro name="Knife_Inertia">
        <origin xyz="0 0 0" 
                rpy="0 0 0"/>        
        <mass value="2.900" />
        <inertia  ixx="9.262021396E-02"
                  ixy="4.23043326E-03"
                  ixz="5.458128035E-02"
                  iyy="1.3936975446E-01"
                  iyz="9.51563616E-03"
                  izz="6.341550849E-02" />
    </xacro:macro>

    <xacro:macro name="Camera_Inertia">
        <origin xyz="0 0 0"
                rpy="0 0 0"/>
        <mass value="0.564" />
        <inertia  ixx="0.003881243"
                  ixy="0.0"
                  ixz="0.0"
                  iyy="0.000498940"
                  iyz="0.0"
                  izz="0.003879257" />
    </xacro:macro>

    <xacro:macro name="Camera_Mount_Inertia">
        <origin xyz="0 0 0"
                rpy="0 0 0"/>
        <mass value="0.63617" />
        <inertia  ixx="2.57390373e-03"
                  ixy="0.4E-10"
                  ixz="-6.204366E-04"
                  iyy="2.87452369E-03"
                  iyz="-7.39E-09"
                  izz="5.2167513E-04" />
    </xacro:macro>

