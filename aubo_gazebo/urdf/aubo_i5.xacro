  <!--////////////////////////////////CameraMount_Joint/////////////////////////////////-->
  <joint name="knife_camera_mount_joint" type="fixed">
    <origin xyz="0.0 0.0 0.0001" rpy="0.00 0.0 0.0277"/>
    <parent link="wrist3_Link"/>
    <child link="camera_mount_link"/>
  </joint>

  <!--////////////////////////////////camera/////////////////////////////////-->
  <link name="camera_link">
