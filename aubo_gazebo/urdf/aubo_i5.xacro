      </inertial>
  </link>
  <!--////////////////////////////////camera_joint/////////////////////////////////-->
  <joint name="camera_joint" type="fixed">
    <origin xyz="0.03358 0.098 -0.05518" rpy="-1.5707 -1.5707 0"/>
