      <origin xyz="0.0 0.0 0.0" rpy="0 0 0"/>
      <geometry>
         <mesh filename="package://aubo_description/meshes/knife.STL" />
      </geometry>
      <!--material name="aluminum"/-->
    </visual>
    <collision>
      <origin xyz="0.0 0.0 0.0" rpy="0 0 0"/>
